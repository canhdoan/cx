package bc

var a [10]i32

func sort() {
	for i := 1; i < len(a); i++ {
        value := a[i]
        j := i - 1
		
        for ; j >= 0 && a[j] > value; {
            a[j+1] = a[j]
            j = j - 1
        }
        a[j+1] = value
    }
}

func printA() {
	var display str
	for i := 0; i < len(a); i++ {
		display = str.concat(display, i32.str(a[i]))
		display = str.concat(display, "  ")
	}

	str.print(display)
}

func insertionSort() {
