package bc

func dot(x []i32, y []i32) (r i32) {
	if i32.uneq(len(x), len(y)) {
		r = 0
		return
	}

	var tmp i32
	l := len(x)
	for i := 0; i < l; i++ {
		tmp = i32.mul(x[i], y[i])
		r += tmp
	}
}

func incDotProduct() {
	var v1 []i32
	v1[0] = 1
	v1[1] = 3
	v1[2] = -5
	
	var v2 []i32
	v2[0] = 4
	v2[1] = -2
	v2[2] = -1

	d := dot(v1, v2)
	i32.print(d)
}

